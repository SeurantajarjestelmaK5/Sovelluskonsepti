{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "@/firebase/*": ["firebase/*"],
      "@/components/*": ["components/*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
, "app/(settings)/index.tsx"  ]
}
